//Switch simple
//Salida: 3.

int main{

	int a = 3;
 
	switch (a) {
		case 1:
                        print 1;
                        print '\n';
		case 30: 
                        print 30;
                        print '\n';
		case 3: 
                        print 3;
                        print '\n';
		default:
                        print 10;
                        print '\n';
	}

}
