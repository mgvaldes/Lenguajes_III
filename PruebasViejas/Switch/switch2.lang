//Switch simple con booleanos
//Salida: 10.

int main{

	bool a = true && false;
        print a;
        print '\n';
 
	switch (a) {
		case true:
                        print 1;
                        print '\n';
		case true: 
                        print 30;
                        print '\n';
		default:
                        print 10;
                        print '\n';
	}

}
